{
  "name": "react-setup-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host --open",
    "start": "vite --host --open",
    "build": "vite build && cd build && zip -rq ../build.zip . && cd ..",
    "analyze": "vite build --analyze",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "check": "prettier --check .",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "lint:check": "eslint src/**/*.{js,jsx}",
    "lint:fix": "eslint src --fix",
    "eslint": "eslint --fix src/app/**/*.{js,jsx}",
    "lint:strict": "eslint src --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.11",
    "dotenv": "^17.2.1",
    "esbuild-plugin-react-virtualized": "^1.0.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwindcss": "^4.1.11",
    "vite-plugin-clean": "^2.0.1",
    "vite-plugin-compression2": "^2.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "vite": "^7.0.4",
    "@babel/core": "^7.25.2",
    "@babel/eslint-parser": "^7.27.5",
    "@babel/preset-react": "^7.24.7",
    "@types/node": "^20.14.10",
    "autoprefixer": "^10.4.21",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.37.5",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "rollup-plugin-analyzer": "^4.0.0",
    "vite-plugin-clean": "^2.0.1",
    "vite-plugin-compression2": "^2.2.0",
    "vite-plugin-eslint": "^1.8.1"
  }
}
